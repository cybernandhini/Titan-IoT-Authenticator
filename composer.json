{
  "name": "cybernandhini/titan-iot-authenticator",
  "description": "Titan IoT Authenticator for SMEs with Argon2, OTP, and Role-based Access",
  "type": "project",
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "twilio/sdk": "^6.44"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
